CREATE TABLE TEAM (
    T_ID        VARCHAR2(6) NOT NULL PRIMARY KEY,
    T_NAME      VARCHAR2(30),
    S_ID        VARCHAR2(30) NOT NULL,
    CONFERENCE  VARCHAR2(15),
    DIVISION    VARCHAR2(15),
    DIRECTOR    VARCHAR2(30),
    RANK        NUMBER(2,0),
    CONSTRAINT FK_TEAM FOREIGN KEY(S_ID) REFERENCES STADIUM(S_ID)
);

