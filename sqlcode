alter session set nls_date_format='YYYY-MM-DD';

CREATE TABLE PLAYER(
    P_ID        VARCHAR2(9) NOT NULL PRIMARY KEY,
    P_NAME      VARCHAR2(30),
    POSITION    VARCHAR2(2),
    BACKNUMBER  NUMBER(2,0),
    BIRTH       DATE,
    COLLEGE     VARCHAR2(20),
    DRAFT       NUMBER(4,0),
    T_ID        VARCHAR2(6) NOT NULL,
    CONSTRAINT FK_PLAYER FOREIGN KEY(T_ID) REFERENCES TEAM(T_ID)
);

