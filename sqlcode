CREATE TABLE STADIUM (
    S_ID        VARCHAR2(9) NOT NULL PRIMARY KEY,
    S_NAME      VARCHAR2(30),
    HOMETOWN    VARCHAR2(30),
    SEAT        NUMBER(5, 0)
); 

