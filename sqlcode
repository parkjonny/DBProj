//여기까지 완료

CREATE TABLE PLAYER(
    P_ID        VARCHAR2(9) NOT NULL PRIMARY KEY,
    P_NAME      VARCHAR(30),
    POSITION    VARCHAR(30),
    BACKNUMBER  NUMBER(2),
    SALARY      VARCHAR2(30),
    BIRTH       NUMBER(8),
    P_HOMETOWN  VARCHAR2(9),
    DEBUT       NUMBER(4),
    FOREIGN KEY (T_ID) references TEAM(T_ID)
); 

